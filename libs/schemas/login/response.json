{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "token",
  "description": "Payload containing session token",
  "required": ["id", "timestamp", "token"],
  "additionalProperties": false,
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "pattern": "[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}"
    },
    "timestamp": {
      "type": "number"
    },
    "token": {
      "type": "string"
    }
  }
}
